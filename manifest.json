{
  "manifest_version": 3,
  "name": "eucm",
  "description": "eucm transfer cookies from a website to Surfly session on the fly",
  "version": "0.0.1",
  "icons": {
    "64": "icons/icon.png"
  },
  "permissions": ["webNavigation", "cookies"],
  "host_permissions": ["*://*/*"],
  "background": {
    "service_worker": "background.js"
  }
}
